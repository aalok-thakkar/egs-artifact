.functor foo(number, symbol):number
.functor goo(symbol, number):number
.functor hoo():symbol

.decl A(x:number)
A(@foo(1,"123")) :- true.
A(@goo("1234",2)) :- true.

.output A

.decl B(x:symbol)
B(@hoo()) :- true.

.output B

// Test functor use within a subloop
.decl C(x:number)
C(r) :- r = @foo(x, y), A(x), B(y).
.output C
